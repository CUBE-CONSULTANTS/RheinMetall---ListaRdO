<mvc:View controllerName="listardo.listardo.controller.Home"
xmlns:mvc="sap.ui.core.mvc"
displayBlock="true"
xmlns:core="sap.ui.core"
xmlns="sap.m"
xmlns:layout="sap.ui.layout">
<Page
class="sapUiResponsivePadding--header sapUiResponsivePadding--footer sapUiResponsivePadding--content"
floatingFooter="true" enableScrolling="false"
showFooter="true" 
>
    <customHeader>
        <core:Fragment fragmentName="listardo.listardo.view.Fragment.Master.Header" />
        
    </customHeader>

    <subHeader>
        <core:Fragment fragmentName="listardo.listardo.view.Fragment.Master.SubHeader" />
    </subHeader>

    <content>
        <core:Fragment fragmentName="listardo.listardo.view.Fragment.Master.filterBar" />
        <core:Fragment fragmentName="listardo.listardo.view.Fragment.Master.treeTable" />
    </content>
    <footer>
        <core:Fragment fragmentName="listardo.listardo.view.Fragment.Master.Footer" />
    </footer>
    </Page>
</mvc:View>
